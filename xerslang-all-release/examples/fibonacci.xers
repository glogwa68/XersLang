// Fibonacci calculator in XersLang
// Computes the nth Fibonacci number

fn fibonacci(n: i64) -> i64 {
    if n <= 1 {
        return n;
    }
    let a = 0;
    let b = 1;
    let i = 2;
    while i <= n {
        let temp = a + b;
        a = b;
        b = temp;
        i = i + 1;
    }
    return b;
}

fn main() -> i64 {
    let result = fibonacci(10);
    print_int(result);  // Should print 55
    return 0;
}
