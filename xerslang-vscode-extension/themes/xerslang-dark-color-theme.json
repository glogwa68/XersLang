  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#64748b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.logical",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c084fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Flow Keywords",
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.return",
        "keyword.control.break",
        "keyword.control.continue"
      ],
      "settings": {
        "foreground": "#c084fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.enum",
        "entity.name.type.module",
        "support.type",
        "support.type.primitive",
        "storage.type.primitive",
        "storage.type.built-in"
      ],
      "settings": {
        "foreground": "#818cf8"
      }
    },
    {
      "name": "Struct and Class Names",
      "scope": [
        "entity.name.type.struct",
        "entity.name.type.class",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "Builtin Functions and Support",
      "scope": [
        "support.function",
        "support.function.builtin",
        "entity.name.function.builtin"
      ],
      "settings": {
        "foreground": "#a78bfa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function.misc"
      ],
      "settings": {
        "foreground": "#67e8f9"
      }
    },
    {
      "name": "Function Declarations",
      "scope": [
        "entity.name.function.definition",
        "meta.definition.function entity.name.function"
      ],
      "settings": {
        "foreground": "#67e8f9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameters variable",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#f0abfc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "variable.other.local"
      ],
      "settings": {
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "variable.other.constant",
        "constant.other",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#fb923c"
      }
    },
    {
      "name": "Enum Members",
      "scope": [
        "variable.other.enummember",
        "constant.other.enum"
      ],
      "settings": {
        "foreground": "#fb923c"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal",
        "constant.numeric.binary"
      ],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "string.template"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Characters",
      "scope": [
        "constant.character",
        "string.quoted.single.char"
      ],
      "settings": {
        "foreground": "#86efac"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape",
        "constant.other.character-class.escape"
      ],
      "settings": {
        "foreground": "#6ee7b7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String Interpolation",
      "scope": [
        "meta.embedded",
        "punctuation.section.embedded",
        "meta.template.expression",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.relational",
        "keyword.operator.assignment",
        "keyword.operator.bitwise",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.section.block",
        "punctuation.section.braces",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly"
      ],
      "settings": {
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Attributes and Decorators",
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator",
        "punctuation.decorator",
        "meta.attribute",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Labels",
      "scope": [
        "entity.name.label",
        "punctuation.definition.label"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "name": "Namespace",
      "scope": [
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "constant.language.boolean",
        "constant.language.true",
        "constant.language.false"
      ],
