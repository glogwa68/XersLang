    "variable.declaration": {
      "foreground": "#e2e8f0",
      "underline": true
    },
    "parameter": {
      "foreground": "#f0abfc",
      "italic": true
    },
    "property": "#e2e8f0",
    "type": "#818cf8",
    "type.declaration": {
      "foreground": "#818cf8",
